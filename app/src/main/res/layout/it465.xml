<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<TextView
				android:id="@+id/textview1"
				android:layout_width="match_parent"
				android:layout_height="60dp"
				android:padding="8dp"
				android:background="#7986CB"
				android:gravity="center_horizontal|center_vertical"
				android:text="Cyber Forensics\n(IT465)"
				android:textSize="16sp"
				android:textStyle="bold|italic"
				android:textColor="#000000"/>
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<ScrollView
					android:id="@+id/vscroll1"
					android:layout_width="match_parent"
					android:layout_height="match_parent">
					<LinearLayout
						android:id="@+id/linear4"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:background="#FFFFFF"
						android:orientation="vertical">
						<TextView
							android:id="@+id/textview2"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:background="#CFD8DC"
							android:gravity="center_horizontal"
							android:text="Syllabus "
							android:textSize="12sp"
							android:textColor="#000000"/>
						<Button
							android:id="@+id/button14"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="IT465"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<TextView
							android:id="@+id/textview3"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:background="#CFD8DC"
							android:gravity="center_horizontal"
							android:text="Notes"
							android:textSize="12sp"
							android:textColor="#000000"/>
						<Button
							android:id="@+id/button18"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Module 1"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button20"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Module 2"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button21"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Module 3"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button22"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Module 4"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button19"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Module 5"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button15"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Module 6"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<TextView
							android:id="@+id/textview4"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:background="#CFD8DC"
							android:gravity="center_horizontal"
							android:text="Textbooks"
							android:textSize="12sp"
							android:textColor="#000000"/>
						<Button
							android:id="@+id/button16"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Button"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button23"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Button"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<TextView
							android:id="@+id/textview6"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:background="#CFD8DC"
							android:gravity="center_horizontal"
							android:text="Model question paper "
							android:textSize="12sp"
							android:textColor="#000000"/>
						<Button
							android:id="@+id/button24"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Button"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
						<Button
							android:id="@+id/button17"
							android:layout_width="190dp"
							android:layout_height="50dp"
							android:padding="8dp"
							android:background="@drawable/buttons"
							android:text="Button"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_gravity="center_horizontal|center_vertical"/>
					</LinearLayout>
				</ScrollView>
			</LinearLayout>
		</LinearLayout>
		<WebView
			android:id="@+id/webview1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"/>
	</LinearLayout>
</LinearLayout>
